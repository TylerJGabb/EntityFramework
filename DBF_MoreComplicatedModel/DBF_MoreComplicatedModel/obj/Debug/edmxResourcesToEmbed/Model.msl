          <ScalarProperty Name="COCDisplaySpec" ColumnName="COCDisplaySpec" />
          <ScalarProperty Name="COCDisplayUnitOfMeasure" ColumnName="COCDisplayUnitOfMeasure" />
          <ScalarProperty Name="COCDisplayVariance" ColumnName="COCDisplayVariance" />
          <ScalarProperty Name="TextIfPass" ColumnName="TextIfPass" />
          <ScalarProperty Name="TextIfFail" ColumnName="TextIfFail" />
