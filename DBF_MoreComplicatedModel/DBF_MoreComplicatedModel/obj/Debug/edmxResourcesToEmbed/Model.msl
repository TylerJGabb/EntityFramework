      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="TestMaps">
      <EntityTypeMapping TypeName="RandomModel.TestMap">
        <MappingFragment StoreEntitySet="TestMap">
          <ScalarProperty Name="Record_Number" ColumnName="Record_Number" />
          <ScalarProperty Name="Test_Number" ColumnName="Test_Number" />
          <ScalarProperty Name="Requirement_Number" ColumnName="Requirement_Number" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="Tests">
      <EntityTypeMapping TypeName="RandomModel.Test">
        <MappingFragment StoreEntitySet="Tests">
          <ScalarProperty Name="Record_Number" ColumnName="Record_Number" />
          <ScalarProperty Name="Description" ColumnName="Description" />
