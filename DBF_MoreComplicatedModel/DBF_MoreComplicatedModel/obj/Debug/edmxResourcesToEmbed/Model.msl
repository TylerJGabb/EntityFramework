          <ScalarProperty Name="Description" ColumnName="Description" />
          <ScalarProperty Name="TestMethod" ColumnName="TestMethod" />
          <ScalarProperty Name="IsNumeric" ColumnName="IsNumeric" />
          <ScalarProperty Name="MinimumInternal" ColumnName="MinimumInternal" />
          <ScalarProperty Name="MaximumInternal" ColumnName="MaximumInternal" />
          <ScalarProperty Name="MinimumExternal" ColumnName="MinimumExternal" />
          <ScalarProperty Name="MaximumExternal" ColumnName="MaximumExternal" />
          <ScalarProperty Name="IsPassFail" ColumnName="IsPassFail" />
          <ScalarProperty Name="IsString" ColumnName="IsString" />
          <ScalarProperty Name="IsPressTest" ColumnName="IsPressTest" />
          <ScalarProperty Name="IsLamTest" ColumnName="IsLamTest" />
          <ScalarProperty Name="IsSlitterTest" ColumnName="IsSlitterTest" />
          <ScalarProperty Name="IsActive" ColumnName="IsActive" />
          <ScalarProperty Name="QuarantineIfFail" ColumnName="QuarantineIfFail" />
          <ScalarProperty Name="SequenceNumber" ColumnName="SequenceNumber" />
          <ScalarProperty Name="IncludeCOA" ColumnName="IncludeCOA" />
